<launch>
  <arg name="BumblebeeConfigurationFolder" default="/home/ryan/currentTesting/stereoOut"/>
  <arg name="datasetInputFolder" default="/home/ryan/DataSets/27Jan2017"/>
  <arg name="outputFolder" default="/home/ryan/git/Output/27Jan2017"/>
  <arg name="publishTransform" default="False"/>
  <node name="dataset" pkg="dataset" type="bumbleDataSet.py" output="screen" args="$(arg datasetInputFolder)" />
